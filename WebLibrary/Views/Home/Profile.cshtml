@model WebLibrary.Models.Users.User

<p align="center">Персональный номер пользователя - @Model.Id</p>
<p align="center">Имя пользователя - @Model.FirstName</p>
<p align="center">Фамилия пользователя - @Model.LastName</p>
<p align="center">Возраст пользователя - @Model.Age</p>
<p align="center">Электронная почта пользователя - @Model.Email</p>
<p align="center">Количество добавленных книг пользователем - @ViewData["TotalBooks"]</p>

<div align="center">
<a asp-controller="Account" asp-action="Logout">
    <input type="button" class="btn btn-default border-dark" value="Выйти из системы" />
</a>
</div>